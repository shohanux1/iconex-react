{
  "name": "iconex-react",
  "version": "0.1.8",
  "description": "React components for iconex icons â€” use in React and Next.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shohanux1/iconex-react.git",
    "directory": "packages/iconex-react"
  },
  "homepage": "https://iconex-home.vercel.app",
  "bugs": "https://github.com/shohanux1/iconex-react/issues",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./icons/*": {
      "import": {
        "types": "./dist/icons/*.d.ts",
        "default": "./dist/icons/*.js"
      },
      "require": {
        "types": "./dist/icons/*.d.cts",
        "default": "./dist/icons/*.cjs"
      }
    },
    "./dynamic": {
      "import": {
        "types": "./dist/dynamicIconImports.d.ts",
        "default": "./dist/dynamicIconImports.js"
      },
      "require": {
        "types": "./dist/dynamicIconImports.d.cts",
        "default": "./dist/dynamicIconImports.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "iconex": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "typescript": "^5.6.0",
    "react": "^19.0.0",
    "@types/react": "^19.0.0",
    "rimraf": "^6.0.0"
  },
  "keywords": ["icons", "react", "svg", "icon-library", "iconex", "nextjs"]
}
